{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
				"apigateway:*",
				"cloudwatch:*",
				"ecr:*",
				"ecs:*",
				"elasticloadbalancing:*",
				"kms:*",
				"lambda:*",
				"logs:*",
				"sns:*"
			],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:RequestedRegion": "us-west-2"
                },
                "NumericLessThan": {
                    "aws:MultiFactorAuthAge": "14400"
                }
            }
        }
	]
}